          </GridPane>
         <Pane prefHeight="70.0" prefWidth="460.0">
            <children>
                <Button mnemonicParsing="false" onAction="#createContactPerson" prefHeight="27.0" prefWidth="460.0" stylesheets="@styles.css" text="Lagre Kontaktperson" />
            </children>
            <VBox.margin>
               <Insets top="15.0" />
            </VBox.margin>
         </Pane>
       </children>
      <padding>
         <Insets left="20.0" right="10.0" />
      </padding>
